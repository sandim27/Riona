{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true
  },
  "rules": {
    "strict": 0,
    "curly": 0,
    "quotes": [2, "single", "avoid-escape"],
    "semi": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": [2, {"args": "all"}],
    "camelcase": [2, {"properties": "never"}],
    "new-cap": 0,
    "accessor-pairs": 0,
    "brace-style": [2, "1tbs"],
    "consistent-return": 0,
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-bitwise": 0,
    "no-multi-spaces": 2,
    "no-shadow": 2,
    "no-unused-expressions": 2,
    "space-after-keywords": 0,
    "space-before-blocks": 2,

  },
  "globals": {
    "inject": false,
    "module": false,
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false,
    "expect": false,
    "window": false,
    "document": false
  },
  "plugins": [
    "react"
  ]
}
